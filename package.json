{
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc & node .",
    "local": "ts-node-dev --respawn --transpile-only ./src/index.ts",
    "dev": "rm -f .env && cp .env.development .env && ts-node-dev --respawn --transpile-only ./src/index.ts",
    "qa": "rm -f .env && cp .env.qa .env && ts-node-dev --respawn --transpile-only ./src/index.ts",
    "uat": "rm -f .env && cp .env.uat .env && ts-node-dev --respawn --transpile-only ./src/index.ts",
    "prod": "rm -f .env && cp .env.production .env && ts-node-dev --respawn --transpile-only ./src/index.ts",
    "tslint": "tslint --project tsconfig.json"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.8.8",
    "prisma": "^5.17.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv-flow": "^3.2.0",
    "@types/morgan": "^1.9.3",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "express-validator": "^7.1.0",
    "i18n-nodejs": "^3.0.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "reflect-metadata": "^0.2.1",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.2.0"
  }
}
